{
  "version": 2,
  "name": "sapphire-modbot-dashboard",
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/clean-dashboard.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/clean-dashboard.html"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/public/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
